#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/jayde/luminis/node_modules/.pnpm/parse-domain@8.2.2/node_modules/parse-domain/bin/node_modules:/mnt/c/Users/jayde/luminis/node_modules/.pnpm/parse-domain@8.2.2/node_modules/parse-domain/node_modules:/mnt/c/Users/jayde/luminis/node_modules/.pnpm/parse-domain@8.2.2/node_modules:/mnt/c/Users/jayde/luminis/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/jayde/luminis/node_modules/.pnpm/parse-domain@8.2.2/node_modules/parse-domain/bin/node_modules:/mnt/c/Users/jayde/luminis/node_modules/.pnpm/parse-domain@8.2.2/node_modules/parse-domain/node_modules:/mnt/c/Users/jayde/luminis/node_modules/.pnpm/parse-domain@8.2.2/node_modules:/mnt/c/Users/jayde/luminis/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../parse-domain/bin/update.js" "$@"
else
  exec node  "$basedir/../parse-domain/bin/update.js" "$@"
fi
